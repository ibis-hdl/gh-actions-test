{
    "version": 8,
    "include": [
        "Common.json"
    ],
    "configurePresets": [
        {
            "name": "compiler.clang",
            "description": "Clang C/C++ language family frontend for LLVM",
            "hidden": true,
            "inherits": [
            ],
            "cacheVariables": {
                "CMAKE_C_COMPILER": "clang",
                "CMAKE_CXX_COMPILER": "clang++"
            }
        },
        {
            "name": "clang.build-type.debug",
            "hidden": true,
            "inherits": "compiler.clang",
            "cacheVariables": {
                "CMAKE_BUILD_TYPE": "Debug"
            }
        },
        {
            "name": "clang.build-type.release",
            "hidden": true,
            "inherits": "compiler.clang",
            "cacheVariables": {
                "CMAKE_BUILD_TYPE": "Release"
            }
        }
    ],
    "buildPresets": [],
    "testPresets": []
}
